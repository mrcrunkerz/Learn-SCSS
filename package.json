{
  "name": "advance-css",
  "version": "1.0.0",
  "description": "Landing Page for Vera",
  "main": "index.js",
  "browserslist": [
    "last 10 versions"
  ],
  "scripts": {
    "watch:sass": "sass sass/main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss style.comp.css",
    "concat:css": "concat-cli -f icon-font.css style.comp.css -o style.concat.css",
    "prefix:css": "postcss style.concat.css --use autoprefixer -o style.prefix.css",
    "compress:css": "sass style.prefix.css style.css --style=compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Verakumar",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concat-cli": "^4.0.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.92.1"
  }
}
